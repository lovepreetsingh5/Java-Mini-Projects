<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane minHeight="400.0" minWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="listt.comboo.listComboController">
   <children>
      <ComboBox fx:id="combo" layoutX="111.0" layoutY="159.0" onAction="#doFill" prefWidth="150.0" />
      <ListView fx:id="listt" layoutX="111.0" layoutY="200.0" onMouseClicked="#doDelOnClick" prefHeight="130.0" prefWidth="150.0" />
      <Button layoutX="274.0" layoutY="200.0" mnemonicParsing="false" onAction="#doShow" text="Show Selected" />
      <Label fx:id="lblSelected" layoutX="66.0" layoutY="355.0" prefHeight="17.0" prefWidth="272.0" text="Label" />
      <Button layoutX="271.0" layoutY="159.0" mnemonicParsing="false" onAction="#doDelete" text="Delete Selected" />
      <ImageView fx:id="prev" fitHeight="114.0" fitWidth="115.0" layoutX="32.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <effect>
            <DropShadow />
         </effect>
         <image>
            <Image url="@fgg.jpg" />
         </image>
      </ImageView>
      <Button layoutX="40.0" layoutY="119.0" mnemonicParsing="false" onAction="#doSelectPic" prefHeight="25.0" prefWidth="99.0" text="Browse" />
   </children>
</AnchorPane>
